    <script src="/vendor/angular-animate/angular-animate.min.js"></script>
    <script src="/vendor/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
    <script src="/vendor/angular-sails/dist/angular-sails.min.js"></script>
    <script src="/vendor/ng-tags-input/ng-tags-input.min.js"></script>
    <script src="/app/app.js"></script>
    <script src="/app/components/hackernews-resource/hackernews-resource-service.js"></script>
    <script src="/app/components/sails-resource/sails-resource-service.js"></script>
    <script src="/app/components/util-service.js"></script>
    <script src="/app/main/main-routes.js"></script>
